{
  "name": "voebiz-boilerplate",
  "version": "0.1",
  "main": "index.js",
  "author": "Softvaro",
  "license": "ISC",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack/webpack.dev.js",
    "lint": "eslint 'src/**'",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest ./src --coverage --testResultsProcessor jest-sonar-reporter --watchAll=false",
    "test:ci": "npm test -- --coverage",
    "test:staged": "npm run test:ci -- --passWithNoTests --no-cache --runInBand",
    "test:watch": "jest ./src --watchAll --coverage=false",
    "build": "webpack --config ./config/webpack/webpack.prod.js",
    "ci:build": "npm run build"
  },
  "dependencies": {
    "@smiles/smiles-ui-kit-react": "^0.6.1",
    "axios": "^0.27.2",
    "cpf-cnpj-validator": "^1.0.3",
    "email-validator": "^2.0.4",
    "i18next": "^19.6.3",
    "launchdarkly-react-client-sdk": "^2.26.0",
    "password-validator": "^5.3.0",
    "query-string": "^6.13.8",
    "react": "^16.3.1",
    "react-dom": "^16.13.1",
    "react-device-detect": "^1.14.0",
    "react-google-recaptcha": "^2.1.0",
    "react-gtm-module": "^2.0.11",
    "react-hook-form": "^7.33.1",
    "react-i18next": "^11.8.3",
    "react-password-checklist": "^1.3.3",
    "react-router-dom": "5.2.0",
    "reactstrap": "8.7.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@testing-library/dom": "^8.7.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/reactstrap": "^8.5.0",
    "@types/react-gtm-module": "^2.0.0",
    "@types/validator": "^13.7.3",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-commit-msg-linter": "^3.2.8",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^4.0.0",
    "jest": "^27.1.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react-refresh": "^0.10.0",
    "sonarqube-scanner": "^2.8.1",
    "sonarqube-verify": "^1.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2",
    "webpack": "^5.55.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0",
    "webpack-merge": "^5.8.0"
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "./reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "eslint './src/**/*.{tsx,ts}' --fix",
      "npm run test"
    ]
  },
  "engines": {
    "node": "12.13.0"
  }
}
